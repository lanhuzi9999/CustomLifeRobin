    <style name="putao_Widget.CirclePageIndicator" parent="@android:style/Widget">
        <item name="centered">@bool/putao_default_circle_indicator_centered</item>
        <item name="fillColor">@color/putao_default_circle_indicator_fill_color</item>
        <item name="orientation">@integer/putao_default_circle_indicator_orientation</item>
        <item name="radius">@dimen/putao_default_circle_indicator_radius</item>
        <item name="snap">@bool/putao_default_circle_indicator_snap</item>
        <item name="strokeColor">@color/putao_default_circle_indicator_stroke_color</item>
        <item name="strokeWidth">@dimen/putao_default_circle_indicator_stroke_width</item>
    </style>

    <style name="putao_AppBaseTheme" parent="@android:style/Theme.Light"></style>

    <!-- 应用主题样式，控制应用基本控件样式，如 -->
    <style name="putao_AppTheme" parent="@style/putao_AppBaseTheme">

        <!-- 背景 -->
        <item name="android:windowBackground">@color/putao_screen_background_light</item>
        <item name="android:windowNoTitle">true</item>
        <!-- button样式 -->
        <item name="android:buttonStyle">@style/putao_ButtonStyle</item>
        <!-- EditText样式 -->
        <item name="android:editTextColor">@color/putao_edit_text</item>
        <item name="android:editTextBackground">@drawable/putao_edit_bg_selector</item>
        <!-- TextView樣式 -->
        <item name="android:textColorPrimary">@color/putao_primary_text_light</item>
        <item name="android:textSize">@dimen/putao_text_primary_size</item>
        <item name="android:textAppearance">@style/putao_TextAppearance</item>
        <item name="android:textAppearanceLarge">@style/putao_TextAppearanceLarge</item>
        <item name="android:textAppearanceMedium">@style/putao_TextAppearanceMedium</item>
        <!-- ListView樣式 -->
        <item name="android:listViewStyle">@style/putao_ListStyle</item>
        <!-- RadioButton样式 -->
        <item name="android:listChoiceIndicatorSingle">@drawable/putao_btn_radio_holo</item>
    </style>

