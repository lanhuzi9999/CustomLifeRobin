<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    
    <so.contacts.hub.widget.CustomViewPager
	    android:id="@+id/main_viewpager"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:layout_marginBottom="@dimen/putao_dialog_head_height"
    />
	
	<LinearLayout
	    android:id="@+id/cp_choose_main_layout"
	 	android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:orientation="vertical"
    	android:layout_alignParentBottom="true">
    	<View style="@style/putao_DividerStyle_Horizontal_Bottom"/>
		<include layout="@layout/putao_yellow_page_map_cp_select_layout" />
	</LinearLayout>


</RelativeLayout>